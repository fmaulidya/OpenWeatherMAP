				{
					"name": "Forecast Jakarta",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Check if the city is Jakarta\", function () {\r",
									"    pm.expect(pm.response.json().city.name).to.equal(\"Jakarta\");\r",
									"});\r",
									"\r",
									"pm.test(\"Check if weather data is available\", function () {\r",
									"    pm.expect(pm.response.json().list).to.be.an('array').that.is.not.empty;\r",
									"});\r",
									"\r",
									"pm.test(\"Check if there are 5 data\", function () {\r",
									"    pm.expect(pm.response.json().list).to.have.lengthOf(5);\r",
									"});\r",
									"pm.test(\"Check if weather data is available in the list\", function () {\r",
									"    pm.expect(pm.response.json().list.every(item => item.hasOwnProperty('weather'))).to.be.true;\r",
									"});\r",
									"pm.test(\"Check if summary is available\", function () {\r",
									"    pm.expect(pm.response.json().list.every(item => item.hasOwnProperty('main'))).to.be.true;\r",
									"});\r",
									"pm.test(\"Check if date is available\", function () {\r",
									"    pm.expect(pm.response.json().list.every(item => item.hasOwnProperty('dt_txt'))).to.be.true;\r",
									"});\r",
									"\r",
									""
								],
